{
  "name": "voice-to-code",
  "displayName": "voice-to-code",
  "description": "Voice-based coding assistant for accessible programming",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "voice-to-code.insertText",
        "title": "Voice To Code: Insert Text(Test)"
      },
      {
        "command": "voice-to-code.insertLoop",
        "title": "Voice to Code: Insert Text"
      },
      {
        "command": "voice-to-code.simulateCommand",
        "title": "Voice to Code: Simulate Command (Type Only)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}